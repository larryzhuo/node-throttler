    "traffic"
  ],
  "dependencies": {
    "ioredis": "^5.2.4",
    "log4js": "^6.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/collections": "^5.1.5",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.7.18",
    "babel-jest": "^29.2.2",
    "eslint": "^7.5.0",
    "f2elint": "^2.2.1",
    "jest": "^29.2.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/larryzhuo/node-throttler.git"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc --watch",
    "f2elint-scan": "f2elint scan",
